  type: http
  url: e:url
  headers:
    traceId: random
    content-type: application/json
  body: |
    {
      "username": "mehrdad",
      "password": "123"
    }
  outputs:
    session: 
      json: body.session
    errors: body.errors[0]
- export:
    absession: