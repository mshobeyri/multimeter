type: test
title: login_and_get_user_info_load
tags: []
import:
  login_and_get_user_info: login_and_get_user_info.mmt
  users: users.csv
inputs:
  username: majid@gmail.com
outputs:
  name: majid
  family: karimi
  age: 12
steps:
  - let:
      counter: 1
  - repeat: 1s
    steps:
      - call: login_and_get_user_info
        id: user_info
        inputs:
          username: i:username
      - js: counter++
      - assert: user_info.name == 'Hellos'
  - print: ${counter}
  - check: xx == yy
