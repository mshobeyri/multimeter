type: api
title: generate session
tags: []
description: returns session information.
inputs:
  - username: c:username
  - password: c:password
  - traceId: c:traceId
outputs:
  - session: c:session
  - errors: c:errors
interfaces:
  - name: http-json
    protocol: http
    method: post
    format: json
    endpoint: e:endpoint
    headers:
      traceId: i:traceId
      content-type: application/json
    body:
      username: i:username
      password: i:password
    outputs:
      session:
        json: body.session
      errors: body.errors[0]
  - name: http-xml
    protocol: http
    format: xml
    endpoint: e:endpoint
    headers:
      traceId: i:traceId
      content-type: application/xml
    body:
      username: i:username
      password: i:password
    outputs:
      session:
        regex: body, /<session>(.*)<\\/session>/
      errors: body.errors[0]